[Unit]
Description=Adisconcurrent
After=network.target nss-lookup.target

[Service]
Type=forking
PIDFile=/var/run/adisconcurrent.pid
ExecStart=/usr/bin/adisconcurrent
ExecStop=-/sbin/start-stop-daemon --quiet --stop --retry TERM/30/QUIT/5 --pidfile /var/run/adisconcurrent.pid
TimeoutStopSec=35

[Install]
WantedBy=multi-user.target